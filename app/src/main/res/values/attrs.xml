<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="label" format="string"/>
    <attr name="value" format="string"/>
    <declare-styleable name="InputField">
        <attr name="label"/>
        <attr name="value"/>
    </declare-styleable>
    <declare-styleable name="TextInputField">
        <attr name="label"/>
        <attr name="value"/>
        <attr name="inputType" format="enum">
            <enum name="text" value="1"/>
            <enum name="number" value="2"/>
            <enum name="textPassword" value="3"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="SelectInputField">
        <attr name="label"/>
        <attr name="value"/>
        <attr name="list" format="reference"/>
    </declare-styleable>
</resources>